{
    "title": "Delicious Salad Dressing for All Leaf Salads",
    "ingredients": [
        "125 ml vinegar",
        "125 ml water",
        "70 g sugar",
        "15 g salt",
        "10 g instant vegetable broth",
        "100 g sour cream",
        "150 g natural yogurt",
        "30 g mustard",
        "125 ml rapeseed oil"
    ],
    "core_ingredients": [
        "vinegar",
