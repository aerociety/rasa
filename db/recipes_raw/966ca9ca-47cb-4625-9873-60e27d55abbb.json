    "title": "Spaghetti mit Bärlauch",
    "ingredients": [
        "500 g Spaghetti",
        "1 bunch Bärlauch",
        "150 g Kochschinken",
        "1 small onion",
        "3 tbsp cream",
        "4 tbsp olive oil",
        "1 tsp vegetable broth",
        "salt and pepper",
        "Parmesan",
        "some dry white wine",
        "1 tbsp butter",
        "1 tomato"
