    "title": "Rote Linsen-Salat mit Roter Bete",
    "ingredients": [
        "200 g red lentils",
        "1 tsp cumin",
        "1 tsp curry powder",
        "350 ml vegetable broth",
        "4 spring onions",
        "½ bunch parsley",
        "400 g cooked beetroot",
        "1 tsp mustard",
        "3 tbsp apple cider vinegar",
        "4 tbsp olive oil",
        "1 tbsp honey",
        "salt and pepper to taste"
    ],
