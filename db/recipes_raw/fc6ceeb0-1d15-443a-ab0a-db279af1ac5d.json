{
    "title": "Portugiesische Reispfanne",
    "ingredients": [
        "1 EL Tomatenmark",
        "300 g Erbsen",
        "250 g Shrimps",
        "200 g Reis",
        "400 g Hähnchenbrustfilet(s) oder Kalbfleisch",
        "1 g Safran",
        "2 EL Mehl",
        "Salz und Pfeffer",
        "3 EL Öl",
        "600 ml Gemüsebrühe",
        "300 g Tomate(n)",
        "400 g Zucchini",
        "1 Knoblauchzehe",
        "Cayennepfeffer"
    ],
