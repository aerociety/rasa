{
    "title": "Tangy Catalina Pasta Salad",
    "ingredients": [
        "16 ounce package elbow macaroni",
        "6 ounce can small shrimp, drained",
        "5 ounce can tuna, drained and flaked",
        "medium green bell pepper, chopped",
        "¾ cup chopped celery",
        "¼ cup chopped onion",
        "4 tablespoons Catalina salad dressing",
        "3 tablespoons mayonnaise",
        "1 ½ tablespoons sweet pickle relish",
        "pinch garlic salt"
    ],
