{
    "title": "Too Easy Peach Cobbler",
    "ingredients": [
        "6 fresh peaches",
        "1 ½ cups white sugar",
        "2 tablespoons all-purpose flour",
        "1 egg",
        "½ cup butter",
        "5 slices white bread"
    ],
    "core_ingredients": [
        "peaches",
        "sugar",
        "flour",
        "egg",
        "butter",
        "bread"
