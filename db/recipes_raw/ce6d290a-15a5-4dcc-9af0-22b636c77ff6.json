{
    "title": "Vegetable Cream Soup with Sherry",
    "ingredients": [
        "2 onions",
        "50g butter",
        "1 liter vegetable broth",
        "2 cartons cream",
        "5 carrots",
        "1 kohlrabi",
        "1 leek",
        "1 small celeriac or 1/2 large",
        "5 potatoes",
        "salt and pepper",
        "nutmeg",
        "6 tbsp sherry, medium",
        "2 egg yolks",
        "1 carton crème fraîche",
        "flat-leaf parsley, finely chopped"
    ],
    "core_ingredients": [
        "onion",
