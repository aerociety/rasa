{
    "title": "Cheddar and Macaroni Salad",
    "ingredients": [
        "1 cup macaroni",
        "¾ cup cubed Cheddar cheese",
        "1 stalk celery, chopped",
        "1 green bell pepper, chopped",
        "½ cup frozen green peas, thawed",
        "⅓ cup chopped onion",
        "¼ cup mayonnaise",
        "¼ cup sour cream",
        "2 tablespoons milk",
        "1 ½ tablespoons sweet pickle relish"
    ],
