{
    "title": "Gyros Rub Seasoning",
    "ingredients": [
        "4 tsp coarse sea salt",
        "3 tsp sweet paprika",
        "3 tsp dried basil",
        "3 tsp dried thyme",
        "3 tsp dried marjoram",
        "3 tsp dried oregano",
        "3 tsp black pepper",
        "3 tsp dried coriander seeds",
        "3 tsp brown sugar",
        "2 tsp onion granules",
        "2 tsp garlic granules",
        "2 tsp cumin",
        "0.5 tsp cinnamon powder"
    ],
    "core_ingredients": [
