{
    "title": "Peach Pear Salsa",
    "ingredients": [
        "1 tablespoon olive oil",
        "½ red onion, diced",
        "2 cloves garlic, minced",
        "4 peaches, pitted and diced",
        "1 pear, peeled, cored, and diced",
        "¼ cup honey",
        "1 teaspoon curry powder",
        "salt and pepper to taste"
    ],
