{
    "title": "Cold Spaghetti",
    "ingredients": [
        "4 ripe tomatoes",
        "3 cloves garlic",
        "1/3 cup chopped fresh basil",
        "1 teaspoon olive oil",
        "1/2 teaspoon white sugar",
        "1 (8 ounce) package uncooked spaghetti",
        "1/2 cup shredded Parmesan cheese"
    ],
    "core_ingredients": [
        "tomatoes",
