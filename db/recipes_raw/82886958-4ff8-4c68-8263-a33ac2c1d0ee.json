{
    "title": "Fresh Fruit Cobbler",
    "ingredients": [
        "1 cup sliced fresh peaches",
        "¾ cup peeled apple slices",
        "¾ cup peeled pear slices",
        "½ cup blueberries",
        "½ cup pitted, sliced cherries",
        "½ cup pitted, sliced plums",
        "¾ cup white sugar",
        "¼ cup milk",
        "1 egg",
        "1 cup all-purpose flour",
        "1 teaspoon baking powder",
        "½ teaspoon salt",
        "½ teaspoon vanilla extract",
        "2 tablespoons butter, melted"
    ],
