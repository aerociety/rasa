{
    "title": "Couscous Salad, Deliciously Spicy",
    "ingredients": [
        "250 g couscous",
        "250 ml vegetable broth or vegetable stock",
        "1 tbsp tomato paste",
        "1 red bell pepper",
        "1 yellow bell pepper",
        "1 can corn",
        "4 spring onions",
        "2 tbsp rice vinegar",
        "3 tbsp olive oil or sunflower oil",
        "1 tbsp red curry paste, vegan",
        "1 tbsp soy sauce",
        "salt and pepper",
        "chili powder",
        "cumin",
        "sugar",
        "flat-leaf parsley",
        "chives or green onion tops"
    ],
