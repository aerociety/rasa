{
    "title": "Djuvec-Reis",
    "ingredients": [
        "½ onion",
        "1 clove garlic",
        "2 tablespoons butter",
        "1 ½ cups long-grain rice",
        "1 can diced tomatoes",
        "2 tablespoons chicken broth powder",
        "3 tablespoons ajvar",
        "2 cups water",
        "1 teaspoon salt",
        "2 pinches pepper",
        "1 teaspoon sweet paprika powder",
        "1 ½ cups peas, frozen or fresh, shelled",
        "½ red bell pepper",
        "1 tablespoon parsley, frozen"
    ],
